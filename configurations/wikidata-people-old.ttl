#####################################
# Queries
#####################################

<https://linked.opendata.cz/resource/knowledge-graph-browser/expansion-query/wikidata/person/relatives/parents> a browser:ExpansionQuery ;
  dct:title "Rodiče člověka" ;
  browser:hasDataset <https://linked.opendata.cz/resource/knowledge-graph-browser/dataset/wikidata> ;
  browser:query """PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX wdab: <https://linked.opendata.cz/resource/vocabulary/knowledge-graph-browser/wikidata/people/>
PREFIX browser: <https://linked.opendata.cz/ontology/knowledge-graph-browser/>
CONSTRUCT {
  ?father a wdab:human ;
    rdfs:label ?fatherLabel ;
    browser:class "human" .
  
  ?mother a wdab:human ;
    rdfs:label ?motherLabel ;
    browser:class "human" .
    
  ?node wdab:father ?father ;
        wdab:mother ?mother .
  
  wdab:father browser:class ?fatherClass .  
  wdab:mother browser:class ?motherClass .  
} WHERE {
  {
    ?node wdt:P22 ?father .
    BIND("father" AS ?fatherClass)
  } UNION {
    ?node wdt:P25 ?mother .
    BIND("mother" AS ?motherClass)
  }

  SERVICE wikibase:label { bd:serviceParam wikibase:language "cs,en". }
  
}""" .

<https://linked.opendata.cz/resource/knowledge-graph-browser/expansion-query/wikidata/person/relatives/children> a browser:ExpansionQuery ;
  dct:title "Děti člověka" ;
  browser:hasDataset <https://linked.opendata.cz/resource/knowledge-graph-browser/dataset/wikidata> ;
  browser:query """PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX wdab: <https://linked.opendata.cz/resource/vocabulary/knowledge-graph-browser/wikidata/people/>
PREFIX browser: <https://linked.opendata.cz/ontology/knowledge-graph-browser/>
CONSTRUCT {
  ?childOfFather a wdab:human ;
    rdfs:label ?childOfFatherLabel ;
    browser:class "human" .
  
  ?childOfMother a wdab:human ;
    rdfs:label ?childOfMotherLabel ;
    browser:class "human" .
    
  ?childOfFather wdab:father ?node .
  ?childOfMother wdab:mother ?node .
  
  wdab:father browser:class ?fatherClass .  
  wdab:mother browser:class ?motherClass .  
} WHERE {
  
  {
    ?childOfFather wdt:P22 ?node .
    BIND("father" AS ?fatherClass)
  } UNION {
    ?childOfMother wdt:P25 ?node .
    BIND("mother" AS ?motherClass)
  }

  SERVICE wikibase:label { bd:serviceParam wikibase:language "cs,en". }
  
}""" .

<https://linked.opendata.cz/resource/knowledge-graph-browser/expansion-query/wikidata/person/relatives/spouses> a browser:ExpansionQuery ;
  dct:title "Manželé nebo manželky člověka" ;
  browser:hasDataset <https://linked.opendata.cz/resource/knowledge-graph-browser/dataset/wikidata> ;
  browser:query """PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX wdab: <https://linked.opendata.cz/resource/vocabulary/knowledge-graph-browser/wikidata/people/>
PREFIX browser: <https://linked.opendata.cz/ontology/knowledge-graph-browser/>
CONSTRUCT {  
  ?spouse a wdab:human ;
    rdfs:label ?spouseLabel ;
    browser:class "human" .
    
  ?node wdab:spouse ?spouse .
  
  wdab:spouse browser:class "spouse" .  
} WHERE { 
  ?node wdt:P26 ?spouse .

  SERVICE wikibase:label { bd:serviceParam wikibase:language "cs,en". }
  
}""" .

